{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "eol-last": ["error", "always"],
        "array-bracket-spacing": ["warn", "always", { "objectsInArrays": false, "arraysInArrays": false }]
    }
}
